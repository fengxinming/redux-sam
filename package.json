{
  "name": "redux-sam",
  "version": "1.1.1",
  "description": "redux-sam is a middleware of Redux for managing state like Vuex.",
  "main": "./index.js",
  "module": "./es/index.js",
  "unpkg": "./umd.js",
  "jsdelivr": "./umd.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development react-app-rewired start",
    "test": "jest --detectOpenHandles --config jest.config.js",
    "build": "cross-env NODE_ENV=production node ./scripts/build.js",
    "docs": "./scripts/docs.sh",
    "watch": "cross-env NODE_ENV=development node ./scripts/watch.js",
    "gh-pages": "git subtree push --prefix=docs origin gh-pages"
  },
  "keywords": [
    "redux",
    "flux",
    "reducer",
    "state"
  ],
  "author": "Jesse Feng",
  "license": "MIT",
  "devDependencies": {
    "@loadable/component": "^5.10.2",
    "antd": "^3.22.1",
    "babel-plugin-import": "^1.12.0",
    "chain-css-loader": "^1.1.3",
    "clrsole": "^1.1.1",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.5.0",
    "fs-extra": "^7.0.1",
    "jest": "^24.0.0",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.4",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-combine": "^1.0.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "todomvc-app-css": "^2.2.0",
    "uglify-js": "^3.4.9",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "celia": "^6.0.3",
    "celia.object": "^1.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-hobby/redux-sam.git"
  },
  "bugs": {
    "url": "https://github.com/react-hobby/redux-sam/issues"
  },
  "homepage": "https://react-hobby.github.io/redux-sam/"
}
